    "counter=6\n",
    "def condis(x):\n",
    "    if x%2 == 0:\n",
    "        return x\n",
    "    else:\n",
    "        return x+1\n",
    "dates=pd.date_range('1/1/2026',periods=counter)\n",
    "attDat = xl(\"A1:A7\",headers=True)\n",
    "attDat2 = xl(\"C2:N7\")\n",
    "valDat = attDat.copy(deep=True)\n",
    "valDat2 = attDat.copy(deep=True)\n",
    "for x in range(0,counter):\n",
    "    valDat[dates[x]] = attDat2[condis(x)].copy(deep=True)\n",
    "\n",
    "valDat.set_index(\"ID\").T"
   ]
